{
  "expo": {
    "name": "Duxin",
    "slug": "Duxin",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/DuXinIconCircle.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DuxinLogoSplashCircle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.neetisthebest.Duxin",
      "supportsTablet": true,
      "buildNumber": "13",
      "associatedDomains": [
        "applinks:duxinapp.com"
      ],
      "icon": "./assets/DuXinIconSquare.png",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photo of mail letter to summarize and translate it."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/DuXinIconCircle.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.neetisthebest.Duxin",
      "versionCode": 18
    },
    "web": {
      "favicon": "./assets/DuxinLogoFaviconCircle.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-secure-store",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "scheme": "duxin",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a4029a8-7292-4a8a-9ad8-460a99504436"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3a4029a8-7292-4a8a-9ad8-460a99504436"
    }
  }
}
